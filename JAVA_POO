    private void btCadastrarActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        
        String nome=tfNome.getText();
        String idade=tfIdade.getText();
        String endereço=tfEndereço.getText();
        String telefone=tfTelefone.getText();
        
        Cliente c = new Cliente(nome,idade,endereço,telefone);
   
       dados.cadastraCliente(c);
      
        lbTotal.setText(dados.numeroDeClientes()  +  "Cliente Cadastrados.");
        
        System.out.println(c);
        
        JOptionPane.showMessageDialog(this, "Cliente Cadastrado com Sucesso!");
        
        tfNome.setText("");
        tfIdade.setText("");
        tfEndereço.setText("");
        tfTelefone.setText("");
      
        
    }                                           

